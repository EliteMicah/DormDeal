{
  "expo": {
    "name": "rebooked",
    "slug": "rebooked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RebookedIcon.png",
    "scheme": "rebooked",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/SplashRebooked.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elitemicah.rebooked",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashRebooked.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elitemicah.rebooked"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/RebookedFavicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7464af44-b096-4b8d-ba97-d0c86736f2b2"
      }
    },
    "owner": "elitemicah"
  }
}
