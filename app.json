{
  "expo": {
    "name": "DormDeal",
    "slug": "DormDeal",
    "version": "1.0.4",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/images/RebookedIcon.png",
    "scheme": "dormdeal",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.elitemicah.DormDeal",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "DormDeal needs access to your camera to take photos of books.",
        "NSPhotoLibraryUsageDescription": "DormDeal needs access to your photo library to select book images."
      }
    },
    "notification": {
      "icon": "./assets/images/RebookedIcon.png",
      "color": "#3b82f6",
      "iosDisplayInForeground": true
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashRebooked.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elitemicah.dormdeal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/RebookedFavicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/RebookedIcon.png",
          "color": "#3b82f6",
          "sounds": []
        }
      ],
      "expo-build-properties",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.elitemicah.dormdeal",
          "enableGooglePay": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e3a564f3-52f5-456a-9aa4-ae6abaadbc44"
      }
    },
    "owner": "elitemicah"
  }
}
