{
  "expo": {
    "name": "DormDeal",
    "slug": "DormDeal",
    "version": "1.0.0",
    "platforms": ["ios"],
    "orientation": "portrait",
    "icon": "./assets/images/RebookedIcon.png",
    "scheme": "dormdeal",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/RebookedIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.elitemicah.DormDeal",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you select images for your listings."
      }
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashRebooked.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elitemicah.rebooked"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/RebookedFavicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-image-picker"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e3a564f3-52f5-456a-9aa4-ae6abaadbc44"
      }
    },
    "owner": "elitemicah"
  }
}
